// header config file for cmake

// enable json output
#cmakedefine USE_JSON

// enable debug mode
#cmakedefine DEBUG @CMAKE_BUILD_TYPE@

// define package name
#cmakedefine PACKAGE_NAME @PACKAGE_NAME@

// define package version
#cmakedefine PACKAGE_VERSION @PROJECT_VERSION@

// define url for bugreport
#cmakedefine PACKAGE_BUGREPORT @PACKAGE_BUGREPORT@